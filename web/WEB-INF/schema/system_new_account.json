{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "account-desc": {
      "type": "string"
    },
    "db-config": {
      "type": "object",
      "properties": {
        "db-user": {
          "type": "string"
        },
        "db-pass": {
          "type": "string"
        },
        "db-name": {
          "type": "string"
        }
      },
      "required": [
        "db-user",
        "db-pass",
        "db-name"
      ]
    },
    "account-code": {
      "type": "string"
    },
    "_func": {
      "type": "string"
    },
    "api-modules": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "provider.config.apiurl": {
            "type": "string"
          },
          "provider.config.apikey": {
            "type": "string"
          },
          "async": {
            "type": "boolean"
          },
          "package": {
            "type": "string"
          },
          "provider": {
            "type": "string"
          },
          "async-config": {
            "type": "object"
          },
          "_callback": {
            "type": "string"
          },
          "provider.config.apisecret": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "async-method": {
            "type": "string"
          }
        },
        "required": [
          "provider.config.apiurl",
          "provider.config.apikey",
          "async",
          "package",
          "provider",
          "async-config",
          "_callback",
          "provider.config.apisecret",
          "name",
          "async-method"
        ]
      }
    }
  },
  "required": [
    "account-desc",
    "db-config",
    "account-code",
    "_func",
    "api-modules"
  ]
}