#Tenant - Schema definitions
1=CREATE TABLE IF NOT EXISTS _solutions_bb_kyc (kyc_seq SERIAL PRIMARY KEY,provider VARCHAR(20) NOT NULL,client_user_id VARCHAR(20) NOT NULL,req JSON NOT NULL,res JSON NOT NULL,created TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP)
2=CREATE TABLE IF NOT EXISTS _solutions_bb_cb (cb_seq SERIAL PRIMARY KEY,provider VARCHAR(20) NOT NULL,client_user_id VARCHAR(20) NOT NULL,req JSON NOT NULL,res JSON NOT NULL,created TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP)
3=CREATE TABLE IF NOT EXISTS _solutions_finance_los_tsi_wf_def (wf_code VARCHAR(6) PRIMARY KEY,wf_name VARCHAR(30) NOT NULL,client_user_id VARCHAR(20) NOT NULL,wf_def JSON NOT NULL,created TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP)
4=CREATE TABLE IF NOT EXISTS _solutions_finance_los_tsi_wf_loan (wf_loan_id integer primary key generated always as identity,wf_code VARCHAR(6) NOT NULL,client_user_id VARCHAR(20) NOT NULL,wf_def JSON NOT NULL,data JSON NOT NULL,state VARCHAR(20) NOT NULL,created TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP)
5=CREATE TABLE IF NOT EXISTS _solutions_finance_los_tsi_wf_history (wf_history_id SERIAL PRIMARY KEY,wf_code VARCHAR(6) NOT NULL,wf_loan_id INTEGER NOT NULL,client_user_id VARCHAR(20) NOT NULL,client_user_name VARCHAR(30),transition VARCHAR(30) NOT NULL,lat FLOAT, long FLOAT, created TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP)
6=CREATE TABLE IF NOT EXISTS _solutions_jobs_candidate_score_def (s_code VARCHAR(6) PRIMARY KEY,s_desc VARCHAR(50) NOT NULL,client_user_id VARCHAR(20) NOT NULL,variables JSON NOT NULL,created TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP)
7=CREATE TABLE IF NOT EXISTS _solutions_jobs_candidate_score_data (s_data_id integer primary key generated always as identity,s_code VARCHAR(6) NOT NULL,client_user_id VARCHAR(20) NOT NULL,score_def JSON NOT NULL,data JSON NOT NULL,score INTEGER NOT NULL,rating VARCHAR(5) NOT NULL,created TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP)
8=ALTER TABLE _solutions_jobs_candidate_score_def ADD column grades JSON NOT NULL